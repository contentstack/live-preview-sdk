{
    "compilerOptions": {
        "target": "ES2020",
        "exactOptionalPropertyTypes": true,
        "resolveJsonModule": true,
        "declaration": true ,
        "declarationMap": true ,
        "lib": ["DOM", "DOM.Iterable", "ES2022"],
        "types": ["vitest/globals"],
        "sourceMap": true ,
        "outDir": "./dist" ,
        "rootDir": "." ,
        "noEmitOnError": false,
        "strict": true ,
        "moduleResolution": "Bundler" ,
        "esModuleInterop": true ,
        "skipLibCheck": true ,
        "forceConsistentCasingInFileNames": true ,
        "jsx": "react-jsx",
        "module": "ESNext"
    },
    "include": ["src"]
}
