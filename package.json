        "@types/jest": "^27.0.1",
        "@types/uuid": "^8.3.1",
        "@typescript-eslint/eslint-plugin": "^4.31.1",
        "@typescript-eslint/parser": "^4.31.1",
        "babel-loader": "^8.0.5",
        "css-loader": "^5.2.2",
        "eslint": "^7.32.0",
        "husky": "^8.0.0",
        "jest": "^27.2.0",
        "jest-fetch-mock": "^3.0.3",
        "mini-css-extract-plugin": "^2.3.0",
        "style-loader": "^2.0.0",
        "ts-jest": "^27.0.5",
        "ts-loader": "^8.1.0",
        "typescript": "^4.4.3",
        "webpack": "^5.53.0",
        "webpack-cli": "^4.8.0",
        "webpack-dev-server": "^4.7.4",
        "webpack-merge": "^5.8.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/contentstack/live-preview-sdk.git"
