{
    "name": "@contentstack/live-preview-utils",
    "version": "1.4.0",
    "types": "dist/src/index.d.ts",
    "description": "Contentstack provides the Live Preview SDK to establish a communication channel between the various Contentstack SDKs and your website, transmitting  live changes to the preview pane.",
    "main": "dist/index.js",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "rimraf dist/ && webpack --config webpack.prod.js",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:watchAll": "jest --watchAll",
        "test:coverage": "jest src --coverage --watchAll --coverageDirectory=\"coverage\"",
        "dev": "webpack --config webpack.dev.js",
        "prepare": "npm run build && husky install",
        "lint": "eslint src",
        "lint:fix": "eslint --fix",
        "prettier": "prettier src --check",
        "prettier:fix": "prettier src --write",
        "format": "npm run prettier:fix && npm run lint:fix",
        "docs": "typedoc --out docs --entryPointStrategy expand ./src"
    },
    "keywords": [
        "live-preview"
    ],
    "author": "Contentstack",
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "^7.2.2",
        "@babel/plugin-transform-runtime": "^7.23.9",
        "@babel/preset-env": "^7.3.1",
        "@babel/preset-react": "^7.23.3",
        "@babel/preset-typescript": "^7.23.3",
        "@contentstack/advanced-post-message": "github:contentstack/adv-post-message#main",
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/preact": "^3.2.3",
        "@types/jest": "^27.0.1",
        "@types/jsdom": "^21.1.6",
        "@types/lodash-es": "^4.17.12",
        "@types/mustache": "^4.2.2",
        "@types/react": "^18.2.57",
        "@types/react-dom": "^18.2.19",
        "@types/uuid": "^8.3.1",
        "@typescript-eslint/eslint-plugin": "^4.31.1",
        "@typescript-eslint/parser": "^4.31.1",
        "babel-loader": "^8.3.0",
        "css-loader": "^5.2.2",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.6.0",
        "husky": "^8.0.0",
        "jest": "^27.2.0",
        "jest-fetch-mock": "^3.0.3",
        "jsdom": "^23.2.0",
        "mini-css-extract-plugin": "^2.3.0",
        "prettier": "^2.8.4",
        "prettier-eslint": "^15.0.1",
        "rimraf": "^5.0.5",
        "style-loader": "^2.0.0",
        "ts-jest": "^27.0.5",
        "ts-loader": "^9.4.2",
        "typedoc": "^0.23.25",
        "typescript": "^4.4.3",
        "webpack": "^5.53.0",
        "webpack-cli": "^4.8.0",
        "webpack-dev-server": "^4.7.4",
        "webpack-merge": "^5.8.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/contentstack/live-preview-sdk.git"
    },
    "dependencies": {
        "@babel/runtime": "^7.23.9",
        "@preact/compat": "^17.1.2",
        "@preact/signals": "^1.2.2",
        "classnames": "^2.5.1",
        "lodash-es": "^4.17.21",
        "mustache": "^4.2.0",
        "preact": "^10.19.5",
        "react": "npm:@preact/compat",
        "react-dom": "npm:@preact/compat",
        "uuid": "^8.3.2"
    },
    "lint-staged": {
        "src/**/*.{ts,css,md}": [
            "npm run prettier:fix"
        ]
    }
}
